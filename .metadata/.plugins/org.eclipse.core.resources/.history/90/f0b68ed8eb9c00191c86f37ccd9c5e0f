package main;

public class Aritimeticos {

	public static void main(String[] args) {
		int n1=10, n2=9, n3 = 5;
		double media = 0;
		
		//adição
		media = n1 + n2 + n3;
		System.out.println(media);
		//subtração
		media = n1 - n2 - n3;
		System.out.println(media);
		//divisão
		media = n1 / n2 / n3;
		System.out.println(media);
		//multiplicação
		media = n1 * n2 * n3;
		System.out.println(media);
		//módulo ou resto
		media = n1 % n2 % n3;
		System.out.println(media);
	}

}
